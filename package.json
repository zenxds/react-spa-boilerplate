{
  "name": "react-spa-boilerplate",
  "version": "0.1.0",
  "description": "react-spa-boilerplate",
  "main": "build/main.js",
  "scripts": {
    "start": "cross-env BABEL_ENV=development webpack-dev-server -w --open --env=development",
    "vendor": "webpack --progress --env=dll",
    "_build": "cross-env BABEL_ENV=production webpack --progress --env=production",
    "build": "npm run vendor && npm run _build",
    "prettier": "prettier --write 'src/**/*.js' 'src/**/*.jsx'",
    "test": "jest ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn test",
      "yarn prettier",
      "git add"
    ]
  },
  "author": {
    "name": ""
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    },
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-react-css-modules": "^3.4.2",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^7.0.2",
    "css-loader": "^1.0.0",
    "dx-mock": "^0.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "jest-transform-css": "^2.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-less": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "react-refresh": "^0.9.0",
    "style-loader": "^0.23.0",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "unused-webpack-plugin": "^2.4.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.43.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@dx/xbee": "^0.5.9",
    "@dx/xpanda": "^0.0.17",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bundle-loader": "^0.5.6",
    "classnames": "^2.2.6",
    "lottie-web": "^5.5.10",
    "mobx": "5",
    "mobx-react": "5",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^4.3.1"
  }
}
