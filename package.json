{
  "name": "react-spa-boilerplate",
  "version": "0.4.0",
  "description": "react-spa-boilerplate",
  "main": "build/main.js",
  "scripts": {
    "start": "cross-env BABEL_ENV=development webpack server --hot --env env=development",
    "vendor": "webpack --progress --env env=dll",
    "_build": "cross-env BABEL_ENV=production webpack --progress --env env=production",
    "build": "npm run vendor && npm run _build",
    "deploy": "scp -r build/* ../server/app/public",
    "prettier": "prettier --write 'src/**/*.js' 'src/**/*.jsx'",
    "test": "jest ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn test",
      "yarn prettier",
      "git add"
    ]
  },
  "author": {
    "name": ""
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules/",
      "src/",
      "test/__mocks__/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@babel|antd|@?ant-|rc-|@dx))"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@utils": "<rootDir>/src/utils",
      "@utils/(.+)": "<rootDir>/src/utils/$1",
      "@constants": "<rootDir>/src/constants",
      "@components/(.+)": "<rootDir>/src/components/$1",
      "@decorators": "<rootDir>/src/decorators"
    },
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.(css|styl|less|sass|scss)$": "<rootDir>/test/__mocks__/file.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/file.js"
    },
    "globals": {
      "API_SERVER_PLACEHOLDER": ""
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@ant-design/compatible": "^5.1.1",
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.9.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "12.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.8.8",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@loadable/component": "^5.15.3",
    "ahooks": "^3.7.10",
    "antd": "^5.6.2",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.8",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0"
  }
}
