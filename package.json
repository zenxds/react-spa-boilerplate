{
  "name": "react-spa-boilerplate",
  "version": "0.4.0",
  "description": "react-spa-boilerplate",
  "main": "build/main.js",
  "scripts": {
    "start": "cross-env BABEL_ENV=development webpack server --hot --env env=development",
    "vendor": "webpack --progress --env env=dll",
    "_build": "cross-env BABEL_ENV=production webpack --progress --env env=production",
    "build": "npm run vendor && npm run _build",
    "deploy": "scp -r build/* ../server/app/public",
    "prettier": "prettier --write 'src/**/*.js' 'src/**/*.jsx'",
    "test": "jest ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn test",
      "yarn prettier",
      "git add"
    ]
  },
  "author": {
    "name": ""
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules/",
      "src/",
      "test/__mocks__/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@babel|antd|@?ant-|rc-|@dx))"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@utils": "<rootDir>/src/utils",
      "@utils/(.+)": "<rootDir>/src/utils/$1",
      "@constants": "<rootDir>/src/constants",
      "@components/(.+)": "<rootDir>/src/components/$1",
      "@decorators": "<rootDir>/src/decorators"
    },
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.(css|styl|less|sass|scss)$": "<rootDir>/test/__mocks__/file.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/file.js"
    },
    "globals": {
      "API_SERVER_PLACEHOLDER": ""
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.12.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/parser": "^7.0.1",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.8.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^8.1.0",
    "prettier": "^3.2.5",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.0",
    "@loadable/component": "^5.16.3",
    "ahooks": "^3.7.10",
    "antd": "4.24.15",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0"
  }
}
