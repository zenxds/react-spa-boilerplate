{
  "name": "react-spa-boilerplate",
  "version": "0.1.0",
  "description": "react-spa-boilerplate",
  "main": "build/main.js",
  "scripts": {
    "start": "cross-env BABEL_ENV=development webpack server --hot --open --env env=development",
    "vendor": "webpack --progress --env=env=dll",
    "_build": "cross-env BABEL_ENV=production webpack --progress --env env=production",
    "build": "npm run vendor && npm run _build",
    "prettier": "prettier --write 'src/**/*.js' 'src/**/*.jsx'",
    "test": "jest ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn test",
      "yarn prettier",
      "git add"
    ]
  },
  "author": {
    "name": ""
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.16.7",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.5.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "autoprefixer": "^10.4.1",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "dx-mock": "^0.1.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.6",
    "jest-transform-css": "^3.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.5",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss-less": "^5.0.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "react-refresh": "^0.11.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "unused-webpack-plugin": "^2.4.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "dependencies": {
    "@dx/icons": "^0.1.0",
    "@dx/xbee": "^4.17.4-release.1",
    "@dx/xpanda": "^0.2.7",
    "@loadable/component": "^5.15.2",
    "axios": "^0.24.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.3.1",
    "lottie-web": "^5.8.1",
    "mobx": "5",
    "mobx-react": "5",
    "moment": "^2.29.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5"
  }
}
